<!DOCTYPE html>
<html>
  <head>
    <title>MembersOnly</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <%= flash[:notice] do %>
  <p class="notice"><%= notice %></p>
  <% end %>
    <%= flash[:alert] do %>
  <p class="alert"><%= alert %></p>
  <% end %>
    <%= yield %>
  </body>
</html>
